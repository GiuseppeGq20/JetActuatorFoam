FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        //type            cyclic;
        type            fixedValue;
        value           uniform 0;
    }

    outlet
    {
        //type            cyclic;
        type            fixedValue;
        value           uniform 0;
    }

    symm
    {
        type            symmetry;
    }

    wall
    {
        type            zeroGradient;
    }

}


