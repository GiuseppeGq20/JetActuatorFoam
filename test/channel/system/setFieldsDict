FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues 
( 
    //volVectorFieldValue U (0 0 0) 
    volScalarFieldValue T 7500 
    //volScalarFieldValue p 1 
);

regions         
( 
    boxToCell 
    { 
        box (5 -100 -100) (100 100 100) ; 
        fieldValues 
        (
            volScalarFieldValue T 7200
            //volScalarFieldValue p 0.1
        ); 
     } 
);

