FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    	volScalarFieldValue T 288
    	volScalarFieldValue p 101325
	    //volScalarFieldValue rho 1.22
 	//volScalarFieldValue mySigma 1e-010    
);

regions
(
    boxToCell
    {
        box (-0.001 0 -0.01) (0.001 0.0004 0.01);
        fieldValues
        (
            volScalarFieldValue T 7500
	        volScalarFieldValue p   1e6
            //volScalarFieldValue rho 1.22             
          //  volScalarFieldValue mySigma 0.0003 
        );
		
    }

);


