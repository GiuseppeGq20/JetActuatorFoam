FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //




application     newFoam;

startFrom       latestTime;

startTime       0;

stopAt      endTime;

endTime        0.000015;

deltaT         5e-9;

writeControl    adjustableRunTime;

writeInterval  1e-7;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable true;

adjustTimeStep yes;

maxCo           0.5;

maxDeltaT       0.9;

