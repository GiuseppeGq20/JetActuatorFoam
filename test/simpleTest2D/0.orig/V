FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            fixedValue;
        value   uniform 0;
    }

    "(electrodSides|electrodFaceTop)"
    {
        type            fixedValue;
        value   uniform 0;
    }
    
    electrodFaceBot
    {
        type            fixedValue;
        value   uniform 1;
    }

    "(front|back)"
    {
        type            empty;
    }

    axis
    {
        type symmetry;
    }
}


