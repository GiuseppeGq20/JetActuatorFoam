FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Tinlet          298;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
    outlet
    {
        type            fixedValue;
        value   uniform $Tinlet;
    }

    "electrod.*"
    {
        type            zeroGradient;
    }

    "(front|back)"
    {
        type            empty;
    }

    axis
    {
        type symmetry;
    }
    
    //#includeEtc "caseDicts/setConstraintTypes"
}

